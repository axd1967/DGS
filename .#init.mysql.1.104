CREATE TABLE FAQlog (
  ID int(11) NOT NULL auto_increment,
  uid int(11) NOT NULL default '0',
  FAQID int(11) NOT NULL default '0',
  Question text,
  Answer text,
  PRIMARY KEY  (ID)
) TYPE=MyISAM;


#
# Table structure for table 'Clock'
